{
    "manifest_version": 3,
    "name": "Simple Tabs Exporter",
    "description": "Exports urls from all open tabs in Chrome to the txt file.",
    "version": "0.9",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup/popup.html",
        "default_title": "Simple Tabs Exporter"
    },
    "host_permissions": ["https://*/*", "http://*/*"],
    "permissions": ["downloads", "tabs"]
  }
